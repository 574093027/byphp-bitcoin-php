{
    "name": "byphp/bitwasp-bitcoin",
    "description": "PHP Bitcoin library with functions for transactions, signatures, serialization, Random/Deterministic ECDSA keys, blocks, RPC bindings",
    "type": "library",
    "homepage": "https://github.com/574093027/byphp-bitcoin-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Color",
            "email": "ijings5251@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BitWasp\\Bitcoin\\": "src/"
        },
        "files": [
            "src/Script/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BitWasp\\Bitcoin\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-mcrypt": "*",
        "paragonie/ecc": "^2.0.1",
        "pleonasm/merkle-tree": "1.0.0",
        "rych/hash_pbkdf2-compat": "~1.0",
        "lastguest/murmurhash": "~1.1",
        "byphp/protobuf-php": "^1.0.0",
        "bitwasp/buffertools": "~0.1",
        "bitwasp/trait": "~0.0",
        "bitwasp/secp256k1-php": "~0.0.7"
    },
    "require-dev": {
        "bitwasp/testing-php": "~0.1",
        "symfony/yaml": "~2.6"
    }
}
